<?xml version="1.0" encoding="windows-1252"?>
<operator>
  <name>org.esa.s2tbx.s24scilws.topoShadow</name>
  <operatorClass>org.esa.snap.core.gpf.operators.tooladapter.ToolAdapterOp</operatorClass>
  <alias>toposhadow</alias>
  <label>toposhadow</label>
  <version>1.1.0</version>
  <description>Topographic Shadow Processor</description>
  <authors/>
  <copyright/>
  <internal>false</internal>
  <autoWriteSuppressed>false</autoWriteSuppressed>
  <menuLocation>Menu/Optical/Thematic Water Processing/S2-4Sci Land and Water Study</menuLocation>
  <preprocessTool>false</preprocessTool>
  <writeForProcessing>false</writeForProcessing>
  <mainToolFileLocation>$PYTHON_BIN</mainToolFileLocation>
  <workingDir>$SEOM_TOPO_DIR</workingDir>
  <templateType>VELOCITY</templateType>
  <template type="file">
    <file>topshadow-template.vm</file>
  </template>
  <progressPattern>Progress\[%\]: (\d{1,3})\.(?:.+)</progressPattern>
  <errorPattern>[\s\S]+(?:UserWarning|Error): (.+)</errorPattern>
  <variables>
    <variable>
      <key>SEOM_TOPO_DIR</key>
      <value>$HOME/.snap/auxdata/s2tbx/s24scilws/toposhadow/</value>
      <isShared>false</isShared>
    </variable>
    <variable>
      <key>SEOM_TOPO_DATA</key>
      <value>$HOME/.snap/auxdata/s2tbx/s24scilws/toposhadow/seom_data/</value>
      <isShared>false</isShared>
    </variable>
    <variable>
      <key>SEOM_TOPO_PYTHON</key>
      <value>$HOME/.snap/auxdata/s2tbx/s24scilws/toposhadow/seom_python/src/</value>
      <isShared>false</isShared>
    </variable>
    <variable>
      <key>PYTHONPATH</key>
      <value>${SEOM_TOPO_DIR}:$PYTHONPATH</value>
      <isShared>false</isShared>
    </variable>
    <variable>
      <key>GDAL_LIBRARY_PATH</key>
      <value>/shared/seom/seom-lot2/COTS/gdal-2.4.0/swig/java</value>
      <isShared>false</isShared>
    </variable>
    <osvariable>
      <key>USERPROFILE</key>
      <value></value>
      <isShared>false</isShared>
      <windows></windows>
      <linux>$HOME</linux>
      <macosx>$HOME</macosx>
      <isTransient>false</isTransient>
    </osvariable>
    <osvariable>
      <key>HOME</key>
      <value></value>
      <isShared>false</isShared>
      <windows>$USERPROFILE</windows>
      <linux></linux>
      <macosx></macosx>
      <isTransient>false</isTransient>
    </osvariable>
    <osvariable>
      <key>PYTHON_BIN</key>
      <value>python</value>
      <isShared>false</isShared>
      <windows>python</windows>
      <linux>python</linux>
      <macosx>python</macosx>
      <isTransient>false</isTransient>
    </osvariable>
  </variables>
  <parameters>
    <parameter>
      <name>targetProductFile</name>
      <dataType>java.io.File</dataType>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <templateparameter>
      <name>postExecuteTemplate</name>
      <dataType>java.io.File</dataType>
      <defaultValue>topshadow-post-template.vm</defaultValue>
      <description/>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>TemplateAfterExecution</parameterType>
      <parameters/>
      <template type="file">
        <file>topshadow-post-template.vm</file>
      </template>
    </templateparameter>
    <parameter>
      <name>outputFile</name>
      <dataType>java.io.File</dataType>
      <defaultValue>/shared/seom/tmp/test</defaultValue>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>ruggedConfFile</name>
      <dataType>java.io.File</dataType>
      <defaultValue>/shared/seom/seom-lot2/seom_data/conf/myRugged.conf</defaultValue>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>verbose</name>
      <dataType>java.lang.Boolean</dataType>
      <description>Verbose mode</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>useFootprint</name>
      <dataType>java.lang.Boolean</dataType>
      <description>Use user defined footprint</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>footprint</name>
      <dataType>java.lang.String</dataType>
      <defaultValue></defaultValue>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>EPSG</name>
      <dataType>java.lang.String</dataType>
      <defaultValue></defaultValue>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>threshold</name>
      <dataType>java.lang.Float</dataType>
      <defaultValue>0.1</defaultValue>
      <description>detection threshold in meters</description>
      <valueSet/>
      <notNull>true</notNull>
      <notEmpty>true</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
  </parameters>
  <source>user</source>
  <isSystem>false</isSystem>
  <isHandlingOutputName>true</isHandlingOutputName>
  <sourceProductDescriptors>
    <org.esa.snap.core.gpf.descriptor.SimpleSourceProductDescriptor>
      <name>sourceProduct</name>
    </org.esa.snap.core.gpf.descriptor.SimpleSourceProductDescriptor>
  </sourceProductDescriptors>
  <targetPropertyDescriptors/>
  <numSourceProducts>1</numSourceProducts>
</operator>